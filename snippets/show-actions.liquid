{% liquid
  assign th_st = settings
  assign show_wishlist = th_st.show_wishlist
  assign show_compare = th_st.show_compare
  assign show_quick_view = th_st.show_quick_view
  assign enable_text_sale_scrolling = th_st.enable_text_sale_scrolling
%}
{%- if product_style == 'bls__product-style-4' %}
  jkljkljkl
  <div class="bls__product-action d-flex space-between border-top pt-10 mt-15 {% if enable_text_sale_scrolling and compare_at_price > price and compare_at_price != blank %}spacing-bottom{% endif %}">
    {%- if show_add_to_cart and sold_out == false and tpl == blank -%}
      <div
        class="bls__product-addtocart bls__product-addtocart-js {{ d_none }}"
        data-product-variant-id="{{ product.selected_or_first_available_variant.id }}"
      >
        <button
          class="bls__js-addtocart btn-secondary border-none button bls_tooltip button--full-width btn-reset"
          type="button"
          name="add"
          role="button"
          aria-label="
            {% if pre_order %}
                {{- 'products.product.pre_order' | t -}}
            {% else %}
                {{- 'products.product.add_to_cart' | t -}}
            {% endif %}
          "
        >
          <span class="icon-plus"></span>
          {%- if pre_order -%}
            <span class="add-cart bls__button-content hidden-loading">
              {{- 'products.product.pre_order' | t -}}
            </span>
          {%- else -%}
            <span class="add-cart bls__button-content hidden-loading">
              {{- 'products.product.add_to_cart' | t -}}
            </span>
          {%- endif -%}
        </button>
      </div>
    {%- endif -%}
    {%- if show_wishlist or show_compare or show_quick_view -%}
      <div class="bls__product-action-inner flex">
        {%- if show_wishlist -%}
          <button
            type="button"
            name="add"
            role="button"
            aria-label="button"
            class="btn-reset bls__product-wishlist bls__product-wishlist-js bls__product-action-btn-js bls_tooltip"
            data-pro-add-wishlist="{{ 'products.product.wishlist' | t }}"
            data-pro-remove-wishlist="{{ 'products.product.browse_wishlist' | t }}"
            data-product-handle="{{ product.handle }}"
          >
            <span class="bls__product-icon origin-style">
              <svg
                width="14"
                height="13"
                viewBox="0 0 14 13"
                fill="none"
                xmlns="http://www.w3.org/2000/svg"
              >
